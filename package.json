    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "tsoa": "^3.11.2",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@kooneko/livereload-webpack-plugin": "^1.2.1",
